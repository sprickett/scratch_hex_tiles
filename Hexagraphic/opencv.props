<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <OpenCVVersion>2410</OpenCVVersion>
    <OpenCV>E:\Code\SDK\OpenCV\opencv$(OpenCVVersion)\build</OpenCV>
    <OpenCVToolset>vc12</OpenCVToolset>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="OpenCVVersion">
      <Value>$(OpenCVVersion)</Value>
    </BuildMacro>
    <BuildMacro Include="OpenCV">
      <Value>$(OpenCV)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="OpenCVToolset">
      <Value>$(OpenCVToolset)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>